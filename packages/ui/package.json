{
  "name": "@dwellpass/ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./*": {
      "import": "./src/*.ts",
      "types": "./src/*.ts"
    },
    "./components/*": {
      "import": "./src/components/*.tsx",
      "types": "./src/components/*.tsx"
    },
    "./components/ui/*": {
      "import": "./src/components/ui/*.tsx",
      "types": "./src/components/ui/*.tsx"
    },
    "./lib/*": {
      "import": "./src/lib/*.ts",
      "types": "./src/lib/*.ts"
    },
    "./hooks/*": {
      "import": "./src/hooks/*.ts",
      "types": "./src/hooks/*.ts"
    },
    "./styles/globals.css": "./src/styles/globals.css",
    "./tailwind.config": "./tailwind.config.ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "lucide-react": "^0.562.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18"
  }
}
