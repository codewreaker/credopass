{
  "name": "dwellpass",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=development bun run.ts",
    "preview": "NODE_ENV=production bun run build && bun server.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun src/server/db/migrate.ts",
    "db:seed": "bun src/server/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "db:setup": "mkdir -p data && bun db:migrate && bun db:seed",
    "db:reset": "rm -rf data/* && bun db:setup",
    "pg:start": "docker-compose -f docker/docker-compose.yml up -d",
    "pg:stop": "docker-compose -f docker/docker-compose.yml down -v"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@electric-sql/pglite": "^0.3.14",
    "@fontsource-variable/inter": "^5.2.8",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@hono/zod-validator": "^0.7.5",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/query-db-collection": "^1.0.6",
    "@tanstack/react-db": "^0.1.60",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.140.5",
    "ag-grid-community": "^35.0.0",
    "ag-grid-react": "^35.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.10.7",
    "lucide-react": "^0.562.0",
    "postgres": "^3.4.7",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-grid-layout": "^2.0.0",
    "recharts": "3.6.0",
    "shadcn": "^3.6.2",
    "std-env": "^3.10.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@redux-devtools/extension": "^3.3.0",
    "@types/bun": "^1.3.3",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.1",
    "vite": "npm:rolldown-vite@7.3.0",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.3.0"
  },
  "engines": {
    "bun": ">=1.3.0"
  }
